{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/server/*": ["./src/server/*"],
      "@/database/*": ["./src/database/*"],
      "@/features/*": ["./src/features/*"],
      "@/interface/*": ["./src/interface/*"],
      "@/tamagui/*": ["./src/tamagui/*"],
      "@/*": ["./src/*"],
      "~assets/*": ["./assets/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
